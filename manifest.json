{
  "manifest_version": 2,
  "name": "Babble",
  "version": "0.1",
  "description": "说都不会话了。",
  "icons": {
    "128": "images/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/js/content.js",
        "dist/js/cryptoutils.js",
        "dist/js/parse.js",
        "dist/js/vendor.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "dist/html/ui.html"
  },
  "permissions": [
    "debugger",
    "storage"
  ]
}
